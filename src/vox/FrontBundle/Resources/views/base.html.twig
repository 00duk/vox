<!DOCTYPE HTML>
<html>
<head>
    <meta charset="utf-8">
    <title>{% block title %}Vox Admin{% endblock %}</title>


    {% stylesheets filter='cssrewrite'
    'bundles/voxadmin/css/bootstrap.min.css'
    'bundles/voxadmin/css/bootstrap-theme.min.css'
    '@voxFrontBundle/Resources/public/css/style.less'
    'http://fonts.googleapis.com/css?family=Exo'
    '//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css'%}
    <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
    {% endstylesheets %}

</head>
<body>


{% block body %}


{% endblock %}

{% block javascript %}
    {% javascripts
    'bundles/voxadmin/js/jquery.js'
    'bundles/voxadmin/js/less.min.js'
    'bundles/voxadmin/js/bootstrap.min.js'%}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

{% endblock %}

</body>
</html>